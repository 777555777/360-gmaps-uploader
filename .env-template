# Environment variables template
# Copy this file to .env and fill in the required values

# Dry run mode will upload images to google but not publish them (for testing)
PUBLIC_DRY_RUN=true

# Google OAuth Client ID
# Create your OAuth Client ID at https://console.developers.google.com/apis/credentials
PUBLIC_GOOGLE_CLIENT_ID=...apps.googleusercontent.com

# MapTiler API Key (for geocoding/search) - Public key with domain restrictions
# Get your free key at https://cloud.maptiler.com/account/keys/
PUBLIC_MAPTILER_API_KEY=your_maptiler_api_key_here
# Enable/disable geocoding search feature (client + server)
PUBLIC_ENABLE_MAP_SEARCH=true

# Deployment (optional - for deploy.sh)
DEPLOY_SERVER_PATH=user@server:/root/docker/apps/pano-publisher
